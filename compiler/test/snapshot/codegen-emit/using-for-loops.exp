Using for loops.

SOURCE:
fn main() {
    {
        let mut sum = 0;

        // this should iterate over values
        for x in [1, 2, 3] {
            sum = sum + x;
        }

        assert_eq(sum, 6);
    }

    {
        let mut sum = 0;

        for (i, x) in [1, 2, 3].enumerate() {
            sum = sum + i + x;
        }

        assert_eq(sum, 9);
    }

    let mut n = 20;

    match true {
        true => {
            n = 25;
        }
        false => (),
    }

    loop {
        if n > 27 {
            break;
        }
        inspect(n);
        n = n + 1;
    }

    n = 0;
    while n < 10 {
        n = n + 1;
    }
    inspect(n);

    let m = Map::new();
    m.insert("a", 1);

    for (k, v) in m {
        inspect(k);
        inspect(v);
    }
}

