fn borgo_main() -> int {
  trait Foo {
    fn bar(x: string) -> int;
  }

  fn check(f: Foo) -> int {
    f.bar("yo")
  }

  struct Baz { x: int }

  impl Baz {
    fn bar(self, _: string) -> int {
      self.x
    }
  }

  check(Baz { x: 1 })
}

